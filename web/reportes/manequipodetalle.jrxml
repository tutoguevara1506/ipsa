	<parameter name="codequipo" class="java.lang.String"/>
	<parameter name="codman" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[select 
 mm.cod_man, tip.nom_tip as tipoman, mm.det_obs as obsenc, mm.fec_ini, mm.fec_fin, case mm.det_sta when 1 then 'PENDIENTE' when 2 then 'CANCELADO' when 3 then 'EN PROCESO' when 4 then 'FINALIZADO' end as estadoman, 
 (select usu1.det_nom from cat_usu as usu1 where usu1.cod_usu = mm.cod_usu) as responsableman, ifnull(per.nom_per,'') as nomperiod,case mm.flg_ext when 0 then 'INTERNO' when 1 then 'EXTERNO' end as inoutman, 
 mm.cod_alt as wo, equ.des_equ as serie, nequ.nom_equ as nomequ,pai.nom_pai as nompai, mm.obs_tec as cliente 
from tbl_mae_man as mm
left join lis_equ as equ on mm.cod_lis_equ = equ.cod_lis_equ
left join cat_equ as nequ on equ.cod_equ = nequ.cod_equ
left join cat_tip as tip on mm.cod_tip = tip.cod_tip
left join cat_per as per on mm.cod_per = per.cod_per
left join cat_pai as pai on equ.cod_pai = pai.cod_pai
where mm.cod_lis_equ = $P{codequipo}  and mm.cod_man = $P{codman}
order by mm.cod_man]]>
	</queryString>
