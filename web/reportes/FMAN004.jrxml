	<queryString>
		<![CDATA[SELECT 
nom_pai, 
des_equ, 
cod_pri, 
fec_ini, 
fec_fin, 
(select det_nom from cat_usu as tecni where tecni.cod_usu= mm.cod_usu) as tecnico, 
(select det_nom from cat_usu as superv where superv.cod_usu=mm.cod_sup) as supervisor, 
dep.nom_dep, 
mm.turno, 
det_obs as falla_reportada,concat(mm.obs_tec ,' ',
ifnull((select 
group_concat(concat(ifnull(ct.nom_fal,''),' ',mf.det_obs) separator ',')
as Resultado 
from tbl_det_man_fal as mf
left join cat_fal as ct on mf.cod_fal = ct.cod_fal
WHERE mf.cod_lis_equ = mm.cod_lis_equ AND mf.cod_man = mm.cod_man),'')) as fallas,
if((select count(dmf.cod_gru_fal) from tbl_det_man_fal as dmf where dmf.cod_lis_equ=mm.cod_lis_equ and dmf.cod_man=mm.cod_man and dmf.cod_gru_fal=1)=0,'','X') as meca,
if((select count(dmf.cod_gru_fal) from tbl_det_man_fal as dmf where dmf.cod_lis_equ=mm.cod_lis_equ and dmf.cod_man=mm.cod_man and dmf.cod_gru_fal=2)=0,'','X') as elec,
(select 
group_concat(concat(ifnull(co.nom_ope,''),' ',mg.det_obs) separator ',')
as Resultado 
from tbl_det_man_gen as mg
left join cat_ope as co on mg.cod_ope = co.cod_ope
WHERE mg.cod_lis_equ = mm.cod_lis_equ AND mg.cod_man = mm.cod_man) as tareas,
(select 
group_concat(concat(ifnull(mp.det_can,''),' ',ifnull(cp.nom_pie,'')) separator ',')
as Resultado 
from tbl_det_man_pie as mp
left join cat_pie as cp on mp.cod_pie = cp.cod_pie
WHERE mp.cod_lis_equ = mm.cod_lis_equ AND mp.cod_man = mm.cod_man) as repuestos
FROM tbl_mae_man as mm
left join  lis_equ as lequ on mm.cod_lis_equ = lequ.cod_lis_equ 
left join cat_pai as pai on lequ.cod_pai = pai.cod_pai 
left join cat_persona as persona on mm.cod_usu = persona.cod_usu 
left join cat_dep as dep on dep.cod_dep = mm.cod_dep 
WHERE mm.cod_lis_equ = $P{cod_lis_equ} 
AND mm.cod_man = $P{cod_man}]]>
	</queryString>
